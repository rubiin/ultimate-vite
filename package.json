{
  "name": "vitely",
  "version": "1.5.0",
  "license": "MIT",
  "description": "Vitely, starter vite project with goodies",
  "author": "Rubiin",
  "keywords": [
    "vue",
    "typescript",
    "atomic-design",
    "ddd",
    "unocss",
    "ddd-example",
    "boilerplate",
    "vue-boilerplate",
    "vite"
  ],
  "scripts": {
    "dev": "vite --open --host --mode dev",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint -c .eslintrc.js --ext .js,.vue,.ts, src",
    "lint:fix": "eslint -c .eslintrc.js --fix --ext .js,.vue,.ts, src",
    "format": "prettier . --write",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test:e2e": "cypress open",
    "test:unit": "vitest",
    "start": "serve dist -l 4000",
    "report": "vite build --mode report",
    "stylelint": "stylelint --config stylelintrc.json 'src/**/*.vue'",
    "stylelint:fix": "stylelint --config stylelintrc.json --fix 'src/**/*.vue'"
  },
  "dependencies": {
    "@vueuse/core": "^8.5.0",
    "@vueuse/head": "^0.7.6",
    "axios": "^0.27.2",
    "pinia": "^2.0.17",
    "pinia-plugin-persistedstate": "^1.6.3",
    "vue": "^3.2.36",
    "vue-i18n": "^9.2.0-beta.35",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@iconify-json/bx": "^1.1.3",
    "@iconify-json/carbon": "^1.1.5",
    "@intlify/vite-plugin-vue-i18n": "^3.4.0",
    "@rollup/plugin-strip": "^2.1.0",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.4",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/vue3": "^6.5.4",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/vue": "^6.6.1",
    "@types/jest": "^27.5.1",
    "@types/node": "^18.6.3",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.32.0",
    "@unocss/preset-icons": "^0.44.0",
    "@unocss/preset-typography": "^0.45.1",
    "@unocss/preset-uno": "^0.44.7",
    "@unocss/transformer-directives": "^0.34.1",
    "@unocss/transformer-variant-group": "^0.41.1",
    "@vitejs/plugin-vue": "^2.3.3",
    "autoprefixer": "^10.4.7",
    "cssnano": "^5.1.9",
    "cssnano-preset-default": "^5.2.12",
    "cypress": "^10.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-storybook": "^0.6.1",
    "eslint-plugin-vue": "^9.2.0",
    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "lint-staged": "^13.0.2",
    "postcss": "^8.4.14",
    "postcss-combine-duplicated-selectors": "^10.0.3",
    "postcss-html": "^1.4.1",
    "prettier": "^2.7.1",
    "rollup-plugin-visualizer": "^5.6.0",
    "rtlcss": "^3.5.0",
    "sass": "^1.52.3",
    "serve": "^13.0.2",
    "stylelint": "^14.9.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^25.0.0",
    "typescript": "^4.7.4",
    "unocss": "^0.44.3",
    "unplugin-auto-import": "^0.7.2",
    "unplugin-vue-components": "^0.19.5",
    "vite": "^2.9.9",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-fonts": "^0.4.0",
    "vite-plugin-pwa": "^0.12.3",
    "vite-svg-loader": "^3.3.0",
    "vitest": "^0.16.0",
    "vue-tsc": "^0.38.5"
  },
  "engines": {
    "node": ">= 14",
    "npm": ">= 8"
  },
  "browserslist": [
    "last 2 version",
    "> 0.2%"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{json,md,scss,yaml,yml}": [
      "prettier --write"
    ]
  }
}
