{
  "name": "vitely",
  "version": "1.5.0",
  "license": "MIT",
  "description": "Vitely, starter vite project with goodies",
  "author": "Rubiin",
  "keywords": [
    "vue",
    "typescript",
    "atomic-design",
    "ddd",
    "unocss",
    "ddd-example",
    "boilerplate",
    "vue-boilerplate",
    "vite"
  ],
  "scripts": {
    "dev": "vite --open --host --mode dev",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint -c .eslintrc.js --ext .js,.vue,.ts, src",
    "lint:fix": "eslint -c .eslintrc.js --fix --ext .js,.vue,.ts, src",
    "format": "prettier . --write",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test:e2e": "cypress open",
    "test:unit": "vitest",
    "start": "serve dist -l 4000",
    "report": "vite build --mode report",
    "stylelint": "stylelint --config stylelintrc.json 'src/**/*.vue'",
    "stylelint:fix": "stylelint --config stylelintrc.json --fix 'src/**/*.vue'"
  },
  "dependencies": {
    "@vueuse/core": "^9.7.0",
    "@vueuse/head": "^1.0.22",
    "axios": "^1.2.1",
    "pinia": "^2.0.28",
    "pinia-plugin-persistedstate": "^3.0.1",
    "vue": "^3.2.45",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@iconify-json/bx": "^1.1.5",
    "@iconify-json/carbon": "^1.1.11",
    "@intlify/vite-plugin-vue-i18n": "^7.0.0",
    "@rollup/plugin-strip": "^3.0.2",
    "@storybook/addon-actions": "^6.5.14",
    "@storybook/addon-essentials": "^6.5.14",
    "@storybook/addon-links": "^6.5.14",
    "@storybook/vue3": "^7.0.12",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/vue": "^6.6.1",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.17",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "@unocss/preset-icons": "^0.50.1",
    "@unocss/preset-typography": "^0.49.1",
    "@unocss/preset-uno": "^0.49.4",
    "@unocss/transformer-directives": "^0.52.1",
    "@unocss/transformer-variant-group": "^0.49.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "autoprefixer": "^10.4.13",
    "cssnano": "^5.1.14",
    "cssnano-preset-default": "^5.2.13",
    "cypress": "^12.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-storybook": "^0.6.8",
    "eslint-plugin-vue": "^9.8.0",
    "husky": "^8.0.2",
    "jsdom": "^21.0.0",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.20",
    "postcss-combine-duplicated-selectors": "^10.0.3",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.1",
    "rollup-plugin-visualizer": "^5.8.3",
    "rtlcss": "^4.0.0",
    "sass": "^1.57.0",
    "serve": "^14.1.2",
    "stylelint": "^14.16.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^29.0.0",
    "typescript": "^5.0.2",
    "unocss": "^0.49.1",
    "unplugin-auto-import": "^0.15.0",
    "unplugin-vue-components": "^0.22.12",
    "vite": "^4.0.2",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-fonts": "^0.7.0",
    "vite-plugin-pwa": "^0.14.0",
    "vite-svg-loader": "^4.0.0",
    "vitest": "^0.29.1",
    "vue-tsc": "^1.0.14"
  },
  "engines": {
    "node": ">= 14",
    "npm": ">= 8"
  },
  "browserslist": [
    "last 2 version",
    "> 0.2%"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{json,md,scss,yaml,yml}": [
      "prettier --write"
    ]
  }
}
