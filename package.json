{
  "name": "vitely",
  "version": "1.5.0",
  "license": "MIT",
  "description": "Vitely, starter vite project with goodies",
  "author": "Rubiin",
  "keywords": [
    "vue",
    "typescript",
    "atomic-design",
    "ddd",
    "unocss",
    "ddd-example",
    "boilerplate",
    "vue-boilerplate",
    "vite"
  ],
  "scripts": {
    "dev": "vite --open --host --mode dev",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint -c .eslintrc.js --ext .js,.vue,.ts, src",
    "lint:fix": "eslint -c .eslintrc.js --fix --ext .js,.vue,.ts, src",
    "format": "prettier . --write",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test:e2e": "cypress open",
    "test:unit": "vitest",
    "start": "serve dist -l 4000",
    "report": "vite build --mode report",
    "stylelint": "stylelint --config stylelintrc.json 'src/**/*.vue'",
    "stylelint:fix": "stylelint --config stylelintrc.json --fix 'src/**/*.vue'"
  },
  "dependencies": {
    "@vueuse/core": "^8.1.2",
    "@vueuse/head": "^0.7.5",
    "axios": "^0.26.1",
    "pinia": "^2.0.12",
    "pinia-plugin-persistedstate": "^1.5.1",
    "vue": "^3.2.31",
    "vue-i18n": "^9.2.0-beta.32",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@iconify-json/bx": "^1.1.2",
    "@iconify-json/carbon": "^1.1.2",
    "@intlify/vite-plugin-vue-i18n": "^3.3.1",
    "@rollup/plugin-strip": "^2.1.0",
    "@storybook/addon-actions": "^6.5.0-alpha.50",
    "@storybook/addon-essentials": "^6.5.0-alpha.50",
    "@storybook/addon-links": "^6.5.0-alpha.51",
    "@storybook/vue3": "^6.5.0-alpha.49",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/vue": "^6.5.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "@unocss/preset-icons": "^0.28.0",
    "@unocss/preset-typography": "^0.30.2",
    "@unocss/preset-uno": "^0.29.5",
    "@unocss/transformer-directives": "^0.28.0",
    "@unocss/transformer-variant-group": "^0.28.0",
    "@vitejs/plugin-vue": "^2.2.4",
    "autoprefixer": "^10.4.2",
    "cssnano": "^5.1.5",
    "cssnano-preset-default": "^5.2.3",
    "cypress": "^9.5.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-storybook": "^0.5.7",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^7.0.4",
    "jsdom": "^19.0.0",
    "lint-staged": "^12.3.7",
    "postcss": "^8.4.12",
    "postcss-combine-duplicated-selectors": "^10.0.3",
    "postcss-html": "^1.3.0",
    "prettier": "^2.6.0",
    "rollup-plugin-visualizer": "^5.6.0",
    "rtlcss": "^3.5.0",
    "sass": "^1.49.9",
    "serve": "^13.0.2",
    "storybook-builder-vite": "^0.1.21",
    "stylelint": "^14.5.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended-vue": "^1.3.0",
    "stylelint-config-standard": "^25.0.0",
    "typescript": "^4.6.2",
    "unocss": "^0.29.5",
    "unplugin-auto-import": "^0.6.6",
    "unplugin-vue-components": "^0.18.0",
    "vite": "^2.8.6",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-fonts": "^0.3.0",
    "vite-plugin-pwa": "^0.11.13",
    "vite-svg-loader": "^3.1.2",
    "vitest": "^0.7.6",
    "vue-tsc": "^0.32.1"
  },
  "engines": {
    "node": ">= 14",
    "npm": ">= 8"
  },
  "browserslist": [
    "last 2 version",
    "> 0.2%"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{json,md,scss,yaml,yml}": [
      "prettier --write"
    ]
  }
}
